{
  "name": "@webfanzc/builder",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "peerDependencies": {
    "vite": "catalog:dev"
  },
  "dependencies": {
    "@webfanzc/render": "workspace:*",
    "@webfanzc/shared": "workspace:*",
    "fast-glob": "catalog:dev",
    "pkg-types": "catalog:dev",
    "ts-dedent": "catalog:dev"
  },
  "devDependencies": {
    "tsdown": "catalog:dev"
  }
